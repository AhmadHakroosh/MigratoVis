{
	// ECMAScript 6 valid code
	"esversion": 6,

	// Unused and undefined strictness
	"undef": true,
	"unused": true,

	// Strict equality
	"eqeqeq": true,

	// Maximum errors before stop building
	"maxerr": 10,

	// Forbid var usage
	"varstmt": true,

	// Enabled browser global variables such as window, document etc.
	"browser": true,

	// Enable development mode
	"devel": true,

	// Enable node mode
	"node": true,

	//
	"boss": true,

	//
	"eqnull": true,
	
	//
	"immed": true,
	
	//
	"latedef": true,
	
	//
	"newcap": true,
	
	//
	"noarg": true,
	
	//
	"sub": true,

	//
	"curly": true
}